<?xml version="1.0"?>
<package >
  <metadata>
    <id>BranchPresets</id>
    <version>0.0.0</version>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <tags>Asos Content Modules Branch Preset</tags>
    <description>Rewrites the datasource paths for  renderings when creating an item from a Sitecore branch</description>
    <authors>ASOS</authors>
  </metadata>
  <files>
    <file src="bin\**" target="lib\net45\" exclude="bin\**\*.xml" />
    <file src="bin\**" target="lib\net451\" exclude="bin\**\*.xml" />
    <file src="bin\**" target="lib\net452\" exclude="bin\**\*.xml" />
    <file src="App_Config\Include\**" target="Content\App_Config\Include"/>
  </files>
</package>